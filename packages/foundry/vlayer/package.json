{
  "name": "green-land-ownership-vlayer",
  "module": "prove.ts",
  "type": "module",
  "scripts": {
    "devnet:up": "docker compose --file docker-compose.devnet.yaml up -d",
    "devnet:down": "docker compose --file docker-compose.devnet.yaml down",
    "lint:solidity": "solhint '../src/**/*.sol' --max-warnings 0 && forge fmt ../src --check",
    "lint-fix:solidity": "solhint '../src/**/*.sol' --fix --noPrompt && forge fmt ../src",
    "prove:mainnet": "VLAYER_ENV=mainnet bun run prove.ts",
    "prove:testnet": "VLAYER_ENV=testnet bun run prove.ts",
    "prove:dev": "VLAYER_ENV=dev bun run prove.ts",
    "test-web:dev": "echo \"No tests specified yet\"",
    "test-web:testnet": "echo \"No tests specified yet\"",
    "build": "cd .. && forge build",
    "clean": "cd .. && forge clean",
    "test": "cd .. && forge test -vv",
    "test:verbose": "cd .. && forge test -vvvv",
    "test:gas": "cd .. && forge test --gas-report",
    "test:coverage": "cd .. && forge coverage",
    "deploy:local": "cd .. && forge script script/DeployGreenWebProof.s.sol --broadcast --rpc-url http://localhost:8545",
    "deploy:sepolia": "cd .. && forge script script/DeployGreenWebProof.s.sol --broadcast --rpc-url https://sepolia.optimism.io --verify",
    "deploy:simulate": "cd .. && forge script script/DeployGreenWebProof.s.sol --rpc-url https://sepolia.optimism.io",
    "prove:land": "bun run proveLandOwnership.ts",
    "prove:land:debug": "VLAYER_DEBUG=true bun run proveLandOwnership.ts",
    "anvil": "anvil --host 0.0.0.0 --port 8545",
    "verify:prover": "cd .. && forge verify-contract $PROVER_ADDRESS src/vlayer/GreenWebProofProver.sol:GreenWebProofProver --etherscan-api-key $ETHERSCAN_API_KEY",
    "verify:verifier": "cd .. && forge verify-contract $VERIFIER_ADDRESS src/vlayer/GreenWebVerifier.sol:GreenWebVerifier --etherscan-api-key $ETHERSCAN_API_KEY --constructor-args $(cast abi-encode 'constructor(address)' $PROVER_ADDRESS)",
    "docs": "cd .. && forge doc",
    "setup": "bun install && cd .. && forge install && forge build",
    "dev": "bun run clean && bun run build && bun run test && bun run prove:land"
  },
  "dependencies": {
    "@vlayer/sdk": "1.0.2",
    "viem": "2.27.0",
    "@vlayer/react": "1.0.2"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/bun": "^1.1.6",
    "@types/eslint__js": "^8.42.3",
    "@types/react-dom": "^18.3.1",
    "solhint": "5.0.5"
  },
  "overrides": {
    "viem": "2.27.0"
  }
}